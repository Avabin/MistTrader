{
  "manifest_version": 3,
  "name": "Mistwood Market History Tracker",
  "version": "1.0.1",
  "description": "Captures and stores transaction history and inventory data from Mistwood market",
  "permissions": [
    "storage",
    "downloads",
    "debugger"
  ],
  "host_permissions": [
    "https://mistwood.pl/*"
  ],
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}